let a = [2, 3, 4]

let fixA = a.map( (a) => {
  return a * 2 // ( <div></div> 사용 가능 )
});

= fixA [4, 6, 8]

function createList() {
  let arr = [];
  for (let i = 0; i < 3; i++)
    arr.push(<div>~</div>);
    
  return arr;
}

const db = 'http://' + host + ':' + port + '/' + database;
-> const db = `http://${host}:${port}/${database}`;
